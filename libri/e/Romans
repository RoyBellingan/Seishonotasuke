bottomNav('<a href="chapter_009.htm"><img src="/images/arrow_white_prev.gif" alt="Previous chapter" width="12" height="15" border="0"><\/a>', '<a href="chapter_011.htm"><img src="/images/arrow_white_next.gif" alt="Next chapter" width="12" height="15" border="0"><\/a>', 'ro', '10');
