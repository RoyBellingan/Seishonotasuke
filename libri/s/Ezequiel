      <meta name="description" content="Lea la Biblia en línea: Ezequiel 39:1-29">
       <link rel="stylesheet" href="/s/biblia/default.css" type="text/css">
       <link rel="stylesheet" href="/css/navigation.css" type="text/css">
       <title>Biblia en línea: Ezequiel 39:1-29 Traducción del Nuevo Mundo de las Santas Escrituras - Página oficial de los Testigos de Jehová</title>
      <script language="JavaScript"  type="text/javascript"><!--
          	if (document.getElementById || document.all || document.layers) {
 				document.write('<script language="JavaScript1.2" src="/scripts/rbi8functions.js" type="text/javascript"></script>');
 				document.write('</head><body onLoad="getChapterVerseCounts(document.forms[0].chapterCount.value, document.forms[0].verseCount.value);"><a name="_top_"></a>');
      } else {
    		window.location.href = "http://www.watchtower.org/s/";
 		   var objMsgWindow = window.open( "/s/biblia/v3msg.htm","","top=120,left=180,height=120,width=350,status=no,toolbar=no,menubar=no,location=no,resizable=no,scrollbars=auto");
          if(objMsgWindow.focus){
               objMsgWindow.focus();
           }
	}
//-->
     </script>
<script language="JavaScript1.2" type="text/javascript">
<!-- 
